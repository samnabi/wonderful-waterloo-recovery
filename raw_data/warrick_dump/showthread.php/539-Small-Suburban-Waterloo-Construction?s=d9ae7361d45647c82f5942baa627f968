        				<div class="userinfo_extra">
        					<dl class="userstats">
        						<dt>Join Date</dt> <dd>Jan 2010</dd>
        						<dt>Location</dt> <dd>Waterloo</dd>
        						
        						<dt>Posts</dt> <dd>563</dd>	
        						
        					</dl>
        					
        					
        				</div>
        			
