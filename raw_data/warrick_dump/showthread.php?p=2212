        			<div style="padding: 6px 10px 3px 0;">
                                
                                        <span class="nodecontrols" style="font-size: 11px; font-weight: normal;">
                                                
                                                        <a name="post2607" href="/web/20131110004813/http://www.wonderfulwaterloo.com/showthread.php?t=205&amp;p=2607#post2607" class="postcounter">#18</a><a id="postcount2607" name="18"></a>
                                                
                                                
                                                
                                        </span>
                                        <div style="margin-top: 6px;">
                                       <span class="postdate old">
                                                
                                                        <span class="date">03-06-2010&nbsp;<span class="time">12:51 AM</span></span>
                                                
                                        </span>
                                        </div>
                                
        			</div>
        			</div>
        
