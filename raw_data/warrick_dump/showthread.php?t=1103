  <li class="postbit postbitim postcontainer" id="post_28220">
  	<div class="postdetails_noavatar">
  		<div class="userinfo" style="background:transparent;">
  			<div class="contact">
  				<div class="username_container" style="position:relative; top: 0px; margin-left: 6px;">
  					
  						<a href="/web/20110404075333/http://www.wonderfulwaterloo.com/member.php?u=2" style="font-size:10px;">UrbanWaterloo</a>
  					
                                          
