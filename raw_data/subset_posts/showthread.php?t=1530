  <li class="postbit postbitim postcontainer" id="post_44395">
  	<div class="postdetails_noavatar">
  		<div class="userinfo_noavatar" style="background:transparent; margin-bottom:-12px;">
  
  			<div style="float:right; text-align: right;">
  			<div style="padding: 0px 6px 0px 0;">
                          
                                  <span class="nodecontrols" style="margin-left: 4px; font-size: 10px;">
                                          
                                  </span>
                          
